        "@types/pdf-parse": "^1.1.4",
        "@types/pg": "^8.11.10",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.18.2",
        "@typescript-eslint/parser": "^8.18.2",
        "eslint": "^9.18.0",
        "jest": "^29.7.0",
        "prettier": "^3.4.2",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.7.2"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
