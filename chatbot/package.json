    "license": "MIT",
    "dependencies": {
        "@biomejs/biome": "^2.3.7",
        "@google/genai": "^1.33.0",
        "@types/node": "^22.10.2",
        "awilix": "^10.0.2",
        "axios": "^1.7.9",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-fileupload": "^1.5.1",
        "he": "^1.2.0",
        "pdf-parse": "^1.1.1",
        "pg": "^8.13.1",
        "redis": "^5.9.0",
        "uuid": "^11.0.3",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/express-fileupload": "^1.5.1",
