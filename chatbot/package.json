  "keywords": [
    "chatbot",
    "rag",
    "typescript",
    "gemini",
    "embeddings"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@types/node": "^22.10.2",
    "awilix": "^10.0.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "ioredis": "^5.4.2",
    "pg": "^8.13.1",
    "pdf-parse": "^1.1.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "cors": "^2.8.5",
    "he": "^1.2.0",
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/jest": "^29.5.14",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/he": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
