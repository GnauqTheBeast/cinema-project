{
    "name": "rag-chatbot-typescript",
    "version": "1.0.0",
    "description": "RAG Chatbot",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "test": "jest",
        "format": "biome format --write .",
        "lint": "biome check .",
        "lint:fix": "biome check --write ."
