{
    "name": "analytics-service",
    "version": "1.0.0",
    "description": "Cinema booking analytics and revenue reporting service",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "format": "biome format --write .",
        "lint": "biome check .",
        "lint:fix": "biome check --write .",
        "proto:generate": "mkdir -p src/grpc/generated && protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/grpc/generated --ts_proto_opt=outputServices=grpc-js --ts_proto_opt=esModuleInterop=true ../movie-service/proto/movie.proto"
    },
    "keywords": [
        "analytics",
        "cinema",
        "microservice"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@biomejs/biome": "^2.3.5",
        "@grpc/grpc-js": "^1.9.14",
        "@grpc/proto-loader": "^0.7.10",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "pg": "^8.11.3",
        "redis": "^4.6.12"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.6",
        "@types/pg": "^8.10.9",
        "ts-node-dev": "^2.0.0",
        "ts-proto": "^1.167.3",
        "typescript": "^5.3.3"
    }
}
